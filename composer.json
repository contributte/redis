{
  "name": "contributte/redis",
  "description": "Redis client integration into Nette framework",
  "keywords": [
    "nette",
    "redis",
    "cache",
    "predis"
  ],
  "type": "library",
  "license": "MIT",
  "homepage": "https://github.com/contributte/redis",
  "authors": [
    {
      "name": "Milan Felix Å ulc",
      "homepage": "https://f3l1x.io"
    }
  ],
  "require": {
    "ext-json": "*",
    "php": ">=7.2",
    "nette/di": "^2.4.17 || ^3.0.1",
    "predis/predis": "^1.1.6 || ^2.0.0"
  },
  "suggest": {
    "ext-igbinary": "For better serialization"
  },
  "require-dev": {
    "mockery/mockery": "^1.3.3",
    "nette/caching": "^2.5.0 || ^3.1.3",
    "nette/http": "^2.4.0 || ^3.0.1",
    "ninjify/nunjuck": "^0.4",
    "ninjify/qa": "^0.12",
    "phpstan/phpstan": "^1.0",
    "phpstan/phpstan-deprecation-rules": "^1.0",
    "phpstan/phpstan-nette": "^1.0",
    "phpstan/phpstan-strict-rules": "^1.0",
    "tracy/tracy": "^2.7.0"
  },
  "autoload": {
    "psr-4": {
      "Contributte\\Redis\\": "src"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Tests\\": "tests"
    }
  },
  "minimum-stability": "dev",
  "prefer-stable": true,
  "config": {
    "sort-packages": true,
    "allow-plugins": {
      "dealerdirect/phpcodesniffer-composer-installer": true
    }
  },
  "extra": {
    "branch-alias": {
      "dev-master": "0.6.x-dev"
    }
  }
}
